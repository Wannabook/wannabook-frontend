{
  "name": "wannabook-frontend",
  "version": "1.0.0",
  "description": "This is a repo for Wannabook frontend",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --env.ENVIRONMENT=production",
    "staging": "webpack --mode production --env.ENVIRONMENT=staging",
    "start": "npm run dev",
    "dev": "webpack-dev-server --mode development --env.ENVIRONMENT=development",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Wannabook/wannabook-frontend.git"
  },
  "author": "Wannabook team",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --write --config ./.prettierrc.js --config-precedence file-override",
      "git add"
    ]
  },
  "devDependencies": {
