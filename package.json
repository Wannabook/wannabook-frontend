    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "axios": "0.19.0",
    "history": "4.10.1",
